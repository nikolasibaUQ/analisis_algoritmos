  number={},
  pages={362-363},
  abstract={This paper proposes to build reusable live programming learning objects for improving introductory computer programming learning. The learning objects employ an innovative learning model, called Interactive Learning Model, to provide visualized and interactive programming learning in a perception and practice integrated, cloud-based, learning platform. A prototype learning environment has been implemented and deployed in Google Cloud to test and evaluate the learning outcomes of proposed learning objects and learning model.},
  keywords={Programming;Visualization;Computational modeling;Algorithm design and analysis;Google;Problem-solving;Learning objects;Learning model;Cloud},
  doi={10.1109/ICALT.2011.113},
  ISSN={2161-377X},
  month={July},}@INPROCEEDINGS{8845425,
  author={Yan, Hua and Yang, Bin},
  booktitle={2019 14th International Conference on Computer Science & Education (ICCSE)}, 
  title={Research and Application of a High-efficiency Teaching Framework Based on Smart Classroom}, 
  year={2019},
  volume={},
  number={},
  pages={483-487},
  abstract={Advanced information technology and educational technology are used in Smart Classroom. More and more educators are paying attention to and studying how to combine the two reasonably and exert the best effect. At present, the widely accepted teaching model based on Smart Classroom is "Pre-class + In-class + After-class". The results of practice show that not all the knowledge is suitable for this model and its teaching efficiency can be further improved. In order to make Smart Classroom more efficient, this paper proposes a teaching framework based on Smart Classroom, and designs different teaching models for different types of knowledge under this framework. The results of practice show that the teaching efficiency of the proposed framework is more remarkable.},
  keywords={Big Data;Computational modeling;Online services;Databases;Information technology;Training;Smart Classroom;high-efficiency;teaching model;teaching framework;Flipped Classroom},
  doi={10.1109/ICCSE.2019.8845425},
