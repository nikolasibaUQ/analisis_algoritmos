@INPROCEEDINGS{9666322,
  author={Olugbade, Temitayo and Sagoleo, Roberto and Ghisio, Simone and Gold, Nicolas and De C Williams, Amanda C and De Gelder, Beatrice and Camurri, Antonio and Volpe, Gualtiero and Bianchi-Berthouze, Nadia},
  booktitle={2021 9th International Conference on Affective Computing and Intelligent Interaction Workshops and Demos (ACIIW)}, 
  title={The AffectMove 2021 Challenge - Affect Recognition from Naturalistic Movement Data}, 
  year={2021},
  volume={},
  number={},
  pages={01-05},
  abstract={We ran the first Affective Movement Recognition (AffectMove) challenge that brings together datasets of affective bodily behaviour across different real-life applications to foster work in this area. Research on automatic detection of naturalistic affective body expressions is still lagging behind detection based on other modalities whereas movement behaviour modelling is a very interesting and very relevant research problem for the affective computing community. The AffectMove challenge aimed to take advantage of existing body movement datasets to address key research problems of automatic recognition of naturalistic and complex affective behaviour from this type of data. Participating teams competed to solve at least one of three tasks based on datasets of different sensors types and real-life problems: multimodal EmoPain dataset for chronic pain physical rehabilitation context, weDraw-l Movement dataset for maths problem solving settings, and multimodal Unige-Maastricht Dance dataset. To foster work across datasets, we also challenged participants to take advantage of the data across datasets to improve performances and also test the generalization of their approach across different applications.},
  keywords={Affective computing;Social networking (online);Pain;Conferences;Computational modeling;Sensors;Problem-solving;affective computing;bodily;challenge;datasets;movement},
  doi={10.1109/ACIIW52867.2021.9666322},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{9155960,
  author={Siiman, Leo A. and Rannastu-Avalos, Meeli and Mäeots, Mario},
  booktitle={2020 IEEE 20th International Conference on Advanced Learning Technologies (ICALT)}, 
  title={Developing smart device friendly asymmetric simulations for teaching collaborative scientific inquiry}, 
  year={2020},
  volume={},
  number={},
  pages={130-131},
  abstract={Collaboration is an important feature of scientific practice. Scientists share, discuss and critically argue over different representations of a problem with others when attempting to generate new knowledge. Similarly, collaboration can be beneficial for inquiry-based science learning at school. Successful collaborative learning, however, relies on engaging students in a condition of positive interdependence. In this paper we propose that asymmetric simulations provide an innovative way to establish positive interdependence among students during inquiry learning which will foster the development of their collaboration skills. Asymmetric simulations divide some information or resources necessary to solve a problem unevenly among group members, thereby requiring more equal participation and contribution among all members during collaboration. The asymmetric simulations we developed are available online for free via the Go-Lab portal (golabz.eu) and therefore can be easily integrated into a digital lesson by school teachers. They were created taking into account the small screen sizes of smart devices such as smartphones or tablets, thereby making them convenient for collaborative work in school classrooms.},
  keywords={Collaboration;Task analysis;Computer simulation;Education;Computational modeling;Collaborative work;Smart devices;asymmetric collaboration;interdependence;computer simulations;inquiry-based science;smart devices},
  doi={10.1109/ICALT49669.2020.00045},
  ISSN={2161-377X},
  month={July},}@INPROCEEDINGS{9315851,
  author={Kumar, Naveen and Yadav, Sanjay Kumar and Yadav, Divakar Singh},
  booktitle={2020 Sixth International Conference on Parallel, Distributed and Grid Computing (PDGC)}, 
  title={Similarity Measure Approaches Applied in Text Document Clustering for Information Retrieval}, 
  year={2020},
  volume={},
  number={},
  pages={88-92},
  abstract={In today's world with ever increasing amount of text assets overloaded on web with digitized libraries, sorting out these documents got developed into a feasible need. Document clustering is an important procedure which consequently sorts out huge number of articles into a modest number of balanced gatherings. Document clustering is making groups of similar documents into number of clusters such that documents within the same group with high similarity values among one another and dissimilar to documents from other clusters. Common applications of document Clustering includes grouping similar news articles, analysis of customer feedback, text mining, duplicate content detection, finding similar documents, search optimization and many more. This lead to utilization of these documents for finding required information in a competent and efficient manner. Document clustering required a measurement for evaluating how surprising two given information are. This dissimilarity is often estimated by using some distance measures, for example, Cosine Similarity, Euclidean distance, etc. In our work, we evaluated and analyzed how effective these measures are in partitioned clustering for text document datasets. In our experiments we have used standard K-means algorithm and our results details on six text documents datasets and five most commonly used distance or similarity measures in text clustering.},
  keywords={Clustering algorithms;Mathematical model;Euclidean distance;Computational modeling;Standards;Handheld computers;Grid computing;Document Clustering;information retrieval;Similarity measures;text clustering;partitioned clustering},
  doi={10.1109/PDGC50313.2020.9315851},
  ISSN={2573-3079},
  month={Nov},}@INPROCEEDINGS{10165540,
  author={Jin, Ziwen},
  booktitle={2023 IEEE 3rd International Conference on Information Technology, Big Data and Artificial Intelligence (ICIBA)}, 
  title={Analysis of the Technical Principles of ChatGPT and Prospects for Pre-trained Large Models}, 
  year={2023},
  volume={3},
  number={},
  pages={1755-1758},
  abstract={ChatGPT is a pre-trained model in the field of natural language processing. As a generative model, the technical foundation of ChatGPT is a deep learning model called the "Generative Adversarial Network". The pre-trained large model architecture of ChatGPT can be summarized as "corpus system+pre-training+fine-tuning". Under the combined force of massive data, super large models, and enormous computing power, chatGPT has ushered in the era of universal artificial intelligence and formed a new paradigm of generative AI development. Generative AI products represented by ChatGPT will promote the development and implementation of Artificial Intelligence Generated Content (AIGC), and trigger significant changes in areas such as information acquisition methods and economic and social cost structures},
  keywords={Analytical models;Ethics;Computational modeling;Force;Production;Chatbots;Generative adversarial networks;Generative Artificial Intelligence;Pre-trained Large Models;Large Language Model;ChatGPT;Artificial Intelligence Generated Content},
  doi={10.1109/ICIBA56860.2023.10165540},
  ISSN={},
  month={May},}@INPROCEEDINGS{9465490,
  author={M, Kiruthigha and Jaganathan, Suresh},
  booktitle={2021 5th International Conference on Computer, Communication and Signal Processing (ICCCSP)}, 
  title={Graph Convolutional Model to Diagnose Autism Spectrum Disorder Using Rs-Fmri Data}, 
  year={2021},
  volume={},
  number={},
  pages={1-5},
  abstract={Human brain is a vital organ in the human body which has more than 100 billion nerves. They communicate through synapses. Few important things governed by brain are memory, emotions, creativity, intelligence etc. The human mind has three fundamental functions like figure out, react and relish. These functions of the mind are guided by one native egocentrism or potential rational capacities. Egocentric inclination happens involuntarily. ASD better termed as developmental delays. It can cause challenges in social interaction, behaviour and communication. Diagnosis of Autism includes both psychological and neurological examination. Early detection of the neurological markers with the help of MRI can help in to more accurate diagnosis. A combination of S-MRI (Structural Magnetic Resonance Imaging), fMRI (Functional Magnetic Resonance Imaging) and Rs-fMRI (Resting State- Functional Magnetic Resonance Imaging) can help in learning all characteristics of brain. Full brain connectivity and signal intensities of Region of Interest can be acquired using various atlases. ASD destroys the structure of the functional connectivity between the multiple brain regions. Deep learning methods proves to be successful in classification of imaging data. Provided with multi modal details provided by different MRI procedures, using a deep learning network can provide accurate classification results. Graph Convolutional Neural Networks can help in classifying whether the person has ASD or not. Given a FC matrix of the brain it is really huge to differentiate the significant features. Thus, it is requiring for dimensionality reduction. So, the purpose of using deep learning model using MRI is to identify Autism in early stage and to start with therapies.},
  keywords={Deep learning;Dimensionality reduction;Autism;Computational modeling;Psychology;Medical treatment;Functional magnetic resonance imaging;Autism;Deep Learning;MRI;Graph Convolutional Neural Network},
  doi={10.1109/ICCCSP52374.2021.9465490},
  ISSN={},
  month={May},}@INPROCEEDINGS{9477710,
  author={Yang, Zhijun and Pan, Weilin and Wang, Yang},
  booktitle={2021 IEEE 3rd International Conference on Computer Science and Educational Informatization (CSEI)}, 
  title={Group Cooperative Learning Model and Its Application Based on Group Knowledge Graph}, 
  year={2021},
  volume={},
  number={},
  pages={324-327},
  abstract={At present, group cooperative learning has received widespread attention as an important mode of independent and inquiry learning. However, during the implementation process, many students were not clear about their learning goals and did not like to discuss research with their classmates. It leads to learning to stay on the surface of formality. In order to solve the above problems and improve the effect of group cooperative learning, many researchers have conducted extensive explorations from the perspective of tools and strategies. Among them, the knowledge graph can visualize the relationship between knowledge points. It characterizes the changes in students' cognitive status during the learning process, guides learners to construct knowledge autonomously, and provides the possibility to solve problems such as the confusion of knowledge structure. At the same time, studies have shown that collaborative knowledge construction has a significant promotion effect on teachers' online learning, and is beneficial to teachers' professional development. Technologically supported collaborative knowledge construction can not only promote the efficiency of knowledge acquisition, but also enhance the effect of communication and interaction, and increase the degree of participation in learning. It is an effective method to enhance the depth and breadth of the interaction of students' learning. Therefore, this study combines the construction of knowledge graphs and group cooperative learning to create a group knowledge graph learning tool based on a group to explore its influence on learners' knowledge ability level and interactive characteristics.},
  keywords={Learning systems;Visualization;Computational modeling;Knowledge acquisition;Education;Knowledge based systems;Collaboration;group cooperative learning;knowledge graph;cooperation},
  doi={10.1109/CSEI51395.2021.9477710},
  ISSN={},
  month={June},}@INPROCEEDINGS{5463771,
  author={Chen, Ming-Puu and Chiu, Chiung-Hui and Wu, Cheng-Chih},
  booktitle={2010 Third IEEE International Conference on Digital Game and Intelligent Toy Enhanced Learning}, 
  title={Instructional Simulations for Teaching High School Computer Science Concepts: A Technology Acceptance Perspective}, 
  year={2010},
  volume={},
  number={},
  pages={216-218},
  abstract={The Experiential-based Learning of Computer Science (ELCS) was employed as a pedagogical framework for instructional simulation design supporting high school students to learn abstract concepts of computer science. ELCS aims to provide teachers and high school learners with flexible-to-adopt, ease-of-use, from-concrete-to-abstract, and from-observation-to-experimentation digital materials on computer science concepts and to fulfill the High School Computer Curricula of Taiwan 2010. Various types of digital media, such as graphics, pictures, animations, simulations, instructional games, etc., were employed to implement four types of learning objects, including examples of concrete experiences, contexts for reflective observation, conceptual models for abstract conceptualization and contexts for active experimentation. A quality evaluation of content expert review revealed that the design of ELCS instructional simulations were appropriate for learning computer science concepts as measured from the perspective of technology acceptance. Further efforts were suggested to examine the effectiveness of types of instructional simulations for the target learners.},
  keywords={Computational modeling;Computer simulation;Education;Computer science;Educational institutions;Context modeling;Materials science and technology;Building materials;Computer graphics;Animation},
  doi={10.1109/DIGITEL.2010.46},
  ISSN={},
  month={April},}@INPROCEEDINGS{4126401,
  author={Gao, Ping and Wu, Su and Cheng, Zhonghua},
  booktitle={2007 Annual Reliability and Maintainability Symposium}, 
  title={Improved criticality analysis of railway locomotive components by simulation}, 
  year={2007},
  volume={},
  number={},
  pages={494-498},
  abstract={Because railway locomotives are complex and highly correlated with each other among its components, it is difficult to make Criticality evaluation by using traditional methods. In the present work, a criticality evaluation index model of the locomotive is established and the Monte Carlo simulation method is employed to obtain the criticality priority of the railway locomotive components. Based on the analysis and method, a program for general calculation is developed and applied to the criticality analysis on DF4D locomotive. The case showed that this method is an effective way to evaluate the criticality of the components of the railway locomotive. In addition, the new method of evaluation of criticality is more objective and suitable for practical situations since the analysis, for example, demonstrates that the equipment components with low failure rate have a high criticality priority. Furthermore, the method is convenient by employing Monte Carlo simulation. Therefore, this improved analysis method on the criticality analysis has a practical impact on making decision for the maintenance mode of complex equipment, i.e., railway locomotive.},
  keywords={Rail transportation;Analytical models;Maintenance;Computational modeling;Failure analysis;Stochastic processes;Railway safety;Electric breakdown;Costs;Statistical analysis},
  doi={10.1109/RAMS.2007.328132},
