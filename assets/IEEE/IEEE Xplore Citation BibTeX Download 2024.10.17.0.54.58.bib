  volume={},
  number={},
  pages={1113-1116},
  abstract={This paper describes the process of design and implementation of multimedia foreign language teaching simulation software is an open system, the simulation of the software system can realize multi computer courses in foreign language, practice. Course selection, foreign language selection method using menu, each foreign language simulation with animation, pictures and text forms, which uses the material is in the form of documents, and is easy to realize dynamic adding, modifying, lookup and update.},
  keywords={Education;Mobile communication;Mobile computing;Cloud computing;Computational modeling;Multimedia communication;Foreign language teaching;practice;simulation;Delphi},
  doi={10.1109/CICN.2014.233},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{6617571,
  author={Horváth, László and Rudas, Imre J.},
  booktitle={2013 IEEE 9th International Conference on Computational Cybernetics (ICCC)}, 
  title={Complex adaptive models for the integration of product centered engineering}, 
  year={2013},
  volume={},
  number={},
  pages={107-112},
  abstract={Classical problem in engineering modeling is that product changes and environmental effects on product are hard to propagate in the product model during engineering activities for the lifecycle of products. In order to support change handling, product modeling in current product lifecycle management (PLM) system uses feature and contextual connection definitions to realize self-adaptive characteristics of product model. Actual problems with this model are identified and recent contributions to establish new modeling solutions are introduced in this paper. The main objectives are to enhance self adaptive characteristics of currently applied product models and to extend contextual connections to new features providing higher level knowledge representation capability. Open architecture PLM system representing latest product modeling principles and methods is considered as environment for the research, simulation, and implementation demanded by the proposed extension using new features and contexts.},
  keywords={Adaptation models;Context modeling;Context;Computational modeling;Mathematical model;Adaptive systems},
  doi={10.1109/ICCCyb.2013.6617571},
  ISSN={},
  month={July},}@INPROCEEDINGS{5231605,
  author={Gao, Zan and Zhao, Zhicheng and Liu, Tao and Nan, Xiaoming and Cai, Anni},
  booktitle={2009 International Conference on Computational Intelligence and Natural Computing}, 
  title={On the Influence of Region Mismatch at Training and Testing in Region-Related Concept Detection}, 
  year={2009},
  volume={1},
  number={},
  pages={42-46},
  abstract={A great deal of region-related concept detection algorithms have been proposed so far, but there are few of them concerning about the problem of mismatched regions at training and testing stages. In order to investigate the mismatch problem in region-related concept detection, we introduce three kinds of methods to annotate the datasets, and then conduct experiments on differently annotated training and testing datasets. We find from these experiments that the detection performance is the best when the regions of a region-related concept are well defined and matched during training and testing, or the detection performance will be decreased. Based on these observations, we propose a fusion scheme to combine the results of classifiers trained with datasets which are annotated by different methods. Experiments on Trecvid-2007 test corpus show that the proposed fusion scheme can obtain performance improvement up to 6~12%.},
  keywords={Testing;Concrete;Detection algorithms;Feature extraction;Telephony;Information retrieval;Computational intelligence;Support vector machines;Support vector machine classification;Video sequences;Region-mismatch;Concept;Detection;Fusion;Annotation;SVM;Trecvid},
  doi={10.1109/CINC.2009.42},
  ISSN={},
  month={June},}@INPROCEEDINGS{9701703,
  author={Lai, Pei-Dong and Xiao, Yun},
  booktitle={2021 17th International Conference on Computational Intelligence and Security (CIS)}, 
  title={Analysis of the causes of employment satisfaction of fresh graduates from colleges and universities --- Mediating variables based on school employment services}, 
  year={2021},
  volume={},
  number={},
  pages={570-574},
  abstract={Employment is the biggest livelihood of the people, and how to make college graduates from “employable” to “well employed” has become an important issue in the employment work of universities and education departments. In this paper, we conducted an empirical analysis on the factors influencing employment satisfaction of fresh college graduates in Guangdong Province in 2021 by structural equation modeling, and concluded that college students' learning ability and psychological quality play a positive role in employment satisfaction, school employment service plays a mediating role in the equation. Countermeasures to improve graduates' employment satisfaction are given in response to the analysis findings to promote colleges and universities to improve their employment services.},
  keywords={Analytical models;Employment;Education;Psychology;Mathematical models;Security;Computational intelligence;recent college graduates;school employment services;employment satisfaction;mediating effect},
  doi={10.1109/CIS54983.2021.00124},
