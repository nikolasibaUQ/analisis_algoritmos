@INPROCEEDINGS{7797909,
  author={Suprun, E. and Stewart, R.A. and Sahin, O. and Panuwatwanich, K.},
  booktitle={2016 IEEE International Conference on Industrial Engineering and Engineering Management (IEEM)}, 
