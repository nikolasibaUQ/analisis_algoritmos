  volume={},
  number={},
  pages={17-23},
  abstract={Information technology changes people's way of working, learning and thinking at an amazing speed, which will inevitably lead to comprehensive reform and development in the education field. The epidemic situation of COVID-19 makes online teaching the main teaching mode of “teaching without stopping and learning without stopping”. The application of modern education technology based on “Chaoxing Platform+Tencent Meeting” in teaching process is explored. Based on teaching environment, interactive teaching mode construction, teaching management and evaluation, the teaching design is carried out in combination with the actual characteristics of each teaching link. The application of virtual simulation modern educational technology in the course Circuit Analysis is studied, and the convenience and the intuitiveness brought by virtual simulation technical resources and interactive platform are given full play, and the virtual simulation technology can effectively solve the obscure circuit analysis problem in theoretical teaching. The aim of teaching model reform has been achieved.},
  keywords={Training;Analytical models;Epidemics;Visualization;Distance learning;Computational modeling;Education;Chaoxing Platform;Tencent Meeting;Virtual Simulation Technology;Interactive Learning;Teaching Mode},
  doi={10.1109/ICENIT57306.2022.00012},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{1205164,
