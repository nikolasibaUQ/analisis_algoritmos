  pages={162-165},
  abstract={This paper intent to make a interdisciplinary studies on product creation and communication theories. In the light of the principle of diffusion of Innovation in developing communication theories, it interprets relevant is sues about product innovation with a revolutionary new perspective and sets up diffusion model to bring enlightenment on this topic. In contrast to traditional innovation thinking, this is more than a procedure, and much easier for product designer to hold. Therefore to explore the issue of product innovation is important to diffusion.},
  keywords={Computational intelligence},
  doi={10.1109/ISCID.2009.48},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{5376385,
  author={Zhang, Jian and Zeng, Guang-Zhou},
  booktitle={2009 International Conference on Artificial Intelligence and Computational Intelligence}, 
  title={RA-An Agent BDI Model Based on Role}, 
  year={2009},
  volume={4},
  number={},
  pages={189-193},
  abstract={Role as new abstraction has been gradually used in agent analysis and design research field, and it is generally defined as an entity or a set of properties. The relationship between role and agent is relative independent, and role model is established before agent model. In this paper, we think role and agent is interdependent, agent is carrier of role, and role means what agent can do. When analysis and design agent model, should firstly have agent entity, then have role. We define role as a set of computation specification and function to realize relationship between two agents, establish a mapping between BDI model and role, and define an agent BDI model based on role (RA) using Pi calculus. Based on theory analysis, RA can keep balance among belief, desire and intention, and maintain model stable. In the end, we give an example of RA, example show RA can be conveniently established and used to construct agent goal solving organization.},
  keywords={Object oriented modeling;Calculus;Intelligent agent;Reflection;Educational institutions;Artificial intelligence;Computational intelligence;Computer science;Sociology;Software engineering;role;Agent model;Agent relationship;BDI;Pi calculus},
  doi={10.1109/AICI.2009.135},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9565174,
  author={Ahouandjinou, Mêtowanou H. and Medenou, Daton and Houessouvo, Roland C. and Houdji, M.A. Godwind},
  booktitle={2021 International Conference on Computing, Computational Modelling and Applications (ICCMA)}, 
  title={Smart model of accessibility and mobility of health care}, 
  year={2021},
  volume={},
  number={},
  pages={153-158},
  abstract={Benin’s health system, like all health systems, is faced with the challenge of providing quality, safe and affordable health care. The problems of understaffing of health care personnel compared to the WHO recommendation on the one hand, and the problems of poor distribution of these health care personnel over the extent of the health care system on the other hand, lead us to think about a model of mobility and accessibility of health care in Benin. This work aims at setting up an intelligent algorithm of accessibility and mobility of health care and will allow any patient embarked at any point of the health system to be taken care of automatically by a doctor and a nurse. To achieve this, we have (i) studied the state of the art of healthcare accessibility and mobility, (ii) modeled the healthcare trajectory, (iii) developed an algorithm for the automatic assignment and management of a patient in a healthcare system and simulated it using operations research and graph tools. The work allowed to set up a dynamic algorithm of accessibility to health care and to answer the challenges of the health care systems.},
  keywords={Operations research;Heuristic algorithms;Computational modeling;Medical services;Tools;Trajectory;Personnel;Accessibility;Mobility;Dynamic algorithm;embedded patient;Care trajectory},
  doi={10.1109/ICCMA53594.2021.00032},
  ISSN={},
  month={July},}@INBOOK{9189968,
  author={Printz, Jacques and Krob, Daniel},
  booktitle={System Architecture and Complexity: Contribution of Systems of Systems to Systems Thinking}, 
  title={The Legacy of Norbert Wiener and the Birth of Cybernetics}, 
  year={2020},
  volume={},
  number={},
  pages={5-31},
  abstract={This chapter provides specific details regarding the concerns of the founders of what is now the science of systems or systemics, in order to understand the stakes that still remain extremely relevant today. It proposes to revisit the key event system sciences, or systemics originated, in which the mathematician Norbert Wiener, professor at MIT, has been an emblematic actor. Cybernetics pertains to time‐related actions that are carried out in an unstable environment. In the 1990s–2000s, it became obvious that what the pioneers N. Wiener, J. von Neumann and A. Turing had imagined was beginning to turn into reality. Of the three, only N. Wiener perhaps knew about transistors. Today, we have all the technology to provide good systemics. The chapter provides a brief view of the chronology and the main changes in teaching systems science at MIT.},
  keywords={Airplanes;Trajectory;Presses;Radar;Atmospheric modeling;Mathematical model;Computational modeling},
  doi={10.1002/9781119751519.ch1},
  ISSN={},
  publisher={Wiley},
  isbn={9781119751496},
  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/9189968},}@INPROCEEDINGS{10622486,
  author={Böck, Yannik N. and Boche, Holger and del Toro, Zoe Garcia and Fitzek, Frank H. P.},
  booktitle={ICC 2024 - IEEE International Conference on Communications}, 
  title={Feynman Meets Turing: The Infeasibility of Digital Compilers for Gate-Based Quantum Computing}, 
  year={2024},
  volume={},
  number={},
  pages={2440-2445},
  abstract={We consider the problem of computing gate-circuit approximations of quantum algorithms, i.e., unitary operators, from the perspective of computable (effective) analysis. The scientific community thinks the Solovay-Kitaev theorem a mile-stone in quantum compiling - the task of computing gate-circuit approximations - because it proves the existence of efficient quantum compilers in an analytic sense. However, since we cannot represent unitary operators in a mere analytical way on digital computers, contemporary digital implementations of quantum compiling resort to heuristic numerics and remain below the computational performance engineers hope to realize using the result of Solovay and Kitaev. This paper discusses quantum compiling within a framework of computable analysis, establishing a concept of computable unitary operators for digital computing based on the theory of Turing machines. Particularly, we prove that digital quantum compiling is uncomputable due to the underlying algebraic structure. Finally, we discuss several implications of our findings for heuristic digital implementations of quantum compiling, hinting toward possible research directions to thoroughly understand the relevant bottlenecks.},
  keywords={Digital computers;Quantum computing;Accuracy;Quantum algorithm;Turing machines;Computational modeling;Quantum mechanics;quantum computing;quantum algorithm;quantum compiler;Solovay-Kitaev theorem;Turing machine},
  doi={10.1109/ICC51166.2024.10622486},
  ISSN={1938-1883},
  month={June},}@INPROCEEDINGS{9247834,
  author={Zatuliveter, Yury and Fishchenko, Elena},
  booktitle={2020 13th International Conference "Management of large-scale system development" (MLSD)}, 
  title={Evolution of Large-scale Systems in the Universal Algorithmic Space of Digital Transformation}, 
  year={2020},
  volume={},
  number={},
  pages={1-5},
  abstract={The article presents an analysis of the patterns of development of the global computer environment (GCE) and its impact on the evolution of large-scale systems. The identification and elimination of the fundamental reasons limiting the further expansion of the fields of application of such systems open the way to the formation of a universal algorithmic space for the digital transformation of large-scale systems in the GCE without fundamental restrictions on their sizes.},
  keywords={Sociotechnical systems;Limiting;Digital transformation;Process control;Programming;Aerospace electronics;Large-scale systems;global computer environment;large-scale systems;digital transformation;the integration of network resources;universal algorithmic space},
  doi={10.1109/MLSD49919.2020.9247834},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{9080042,
  author={Mohsin, Syed Muhammad and Aslam, Sheraz and Akber, Syed Muhammad Abrar and Iqbal, Arshad and Waheed, Abdul and Ikram, Amra},
  booktitle={2020 International Conference on Information Science and Communication Technology (ICISCT)}, 
  title={Empowering Cloud of Things with Edge Computing: A Comparative Analysis}, 
  year={2020},
  volume={},
  number={},
  pages={1-6},
  abstract={Number of networks and their sizes in terms of connected devices are increasing very rapidly. These devices are producing massive amount of raw data for different progressive purposes that need to be processed and stored somewhere. Local data storage and complete processing of massive data will not be possible by resource constrained Internet of Things (IoT) enabling devices anymore in the future. Therefore, there is a need of data storage and processing capabilities to meet the desired goals of applications. Cloud computing and edge computing are the possible solution to circumvent limited data storage and computation issues of IoT devices. This paper presents a comparative study of popular architectures in which IoT devices are integrated with cloud. For this purpose, various IoT and Cloud of Things (CoT) architectures have been studied. Strengths and weaknesses of the studied IoT and CoT architectures are comprehensively analyzed on the basis of five comparison parameters namely; resource based segregation of IoT enabling devices, range based segregation of the communication network, segregation of security solutions, cloud connectivity, and edge computing services.},
  keywords={Cloud Computing;Edge Computing;Internet of Things;Device Resources;Cloud of Things;Security.},
  doi={10.1109/ICISCT49550.2020.9080042},
  ISSN={},
  month={Feb},}@ARTICLE{7336669,
