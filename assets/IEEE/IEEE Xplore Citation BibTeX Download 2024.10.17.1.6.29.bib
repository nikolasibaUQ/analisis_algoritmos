  abstract={This paper discusses the urgency and practicability of applying cloud computing to big data in power systems, combining the characteristics of complex power flow calculations and big data applications in power systems, and studying the relationship between power system power flow calculation methods and cloud computing. The important role of cloud computing in power system dispatching, planning, and scientific research is analyzed, and three visualization methods are realized, namely: basic chart visualization, SVG visualization, and map visualization. Among them, for SVG visualization, this paper proposes an SVG visualization display method based on layered thinking.},
  keywords={Cloud computing;Data visualization;Power system stability;Stability analysis;Real-time systems;Safety;Planning;Cloud Computing;Power System;Big Data;Data Mining},
  doi={10.1109/ICSSIT53264.2022.9716515},
  ISSN={},
  month={Jan},}@INPROCEEDINGS{8978231,
