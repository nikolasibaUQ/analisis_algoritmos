@ARTICLE{9931118,
  author={Moreno-Sandoval, Luis Gabriel and Pomares-Quimbaya, Alexandra},
  journal={IEEE Access}, 
  title={Hybrid Onion Layered System for the Analysis of Collective Subjectivity in Social Networks}, 
