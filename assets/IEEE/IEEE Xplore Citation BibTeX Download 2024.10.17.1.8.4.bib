  ISSN={},
  month={June},}@INPROCEEDINGS{897659,
  author={Glas, G.O. and Zola, J.G. and Denazis, J.M.},
  booktitle={30th Annual Frontiers in Education Conference. Building on A Century of Progress in Engineering Education. Conference Proceedings (IEEE Cat. No.00CH37135)}, 
  title={Technical pedagogical investigation on electronic engineering educational methods in the electronic circuits area}, 
  year={2000},
  volume={1},
  number={},
  pages={T4B/8-T4B10 vol.1},
  abstract={This paper arises from an educative investigation made in an electronic circuits course at the University of Buenos Aires. Didactic strategies were designed and started, sustained in three fundamental points: (1) computer simulation, (2) student teamwork, and (3) the guidance of the teacher as an expert, collaborating with the learning of the student. To complete the experience, multimedia software is being developed. The software is intended to support the students' knowledge construction by presenting them with new situations that could take place, helping them to think up different points of view, while promoting interaction with the teachers, with each other, and with their books and papers. The computer simulation in this experience is understood as a tool to help and make the team process easier, while allowing the students to look for their own problem-resolving strategies.},
  keywords={Electronic circuits;Computer simulation;Design engineering;Engineering profession;Books;CD-ROMs;Education;Teamwork;Collaboration;Computational modeling},
  doi={10.1109/FIE.2000.897659},
  ISSN={0190-5848},
  month={Oct},}@INPROCEEDINGS{10229637,
  author={Wu, Xing and Yuan, Xiao},
  booktitle={2023 2nd International Conference on 3D Immersion, Interaction and Multi-sensory Experiences (ICDIIME)}, 
  title={Optimization Algorithm Design of 3D Animation Rendering Based on Virtual Reality}, 
  year={2023},
  volume={},
  number={},
  pages={92-96},
  abstract={With the development of computer technology, people’s thinking mode has also changed dramatically. 3D animation and virtual reality (VR) technology can be seen everywhere around people. With the development of technology, 3D animation and VR technology are applied in more and more fields, such as education, medical treatment and commerce. Under the background of increasing international competition, the update speed of high-tech is changing with each passing day. With the wide application of VR technology by modern people, the 3D animation industry has been further developed. The effective combination of 3D animation scene graphic design and VR technology can further improve the overall perfection of 3D animation scene design. This paper improves the lighting model and shadow rendering algorithm according to the defects of the general lighting rendering algorithm in the application of 3D animation scenes. Based on this, a hybrid 3D scene lighting rendering algorithm is obtained. When optimizing the Cook Torrance lighting model, a geometric attenuation factor algorithm is used, and when improving the shadow rendering algorithm, a light shadow rendering algorithm based on variance shadow is used.},
  keywords={Solid modeling;Visualization;Three-dimensional displays;Art;Computational modeling;Lighting;Virtual reality;Virtual reality;3D animation rendering;optimization algorithm},
  doi={10.1109/ICDIIME59043.2023.00024},
  ISSN={},
  month={June},}@INPROCEEDINGS{738878,
  author={Crews, T.R.},
  booktitle={FIE '98. 28th Annual Frontiers in Education Conference. Moving from 'Teacher-Centered' to 'Learner-Centered' Education. Conference Proceedings (Cat. No.98CH36214)}, 
  title={Instructional technology for cross-disciplinary learning}, 
  year={1998},
  volume={2},
  number={},
  pages={943-948 vol.2},
  abstract={Advances in computer technology and multimedia systems have led to widespread interest in computer-based instruction and learning environments. presentation of material in realistic contexts, thus promoting constructive and generative learning. However the true potential and benefits of these systems are yet to be realized. Significant benefits will be realized when instructional technologies are systematically incorporated into a system of tools that support ongoing thinking and learning activities. This paper discusses one such effort, "Anchored Learning Environments" (ALE), an emerging architecture for integrated curricula instruction and learning. ALE extend well-developed theories of educational practice into the domain of interactive computer-assisted instruction. The resulting computer-supported environments facilitate sustained learning by active learners. A major goal of the ALE architecture is to help students understand the kinds of problems and opportunities that exist in complex, real world environments. A related goal is to help students integrate their knowledge through the repeated exploration of the same situation (macrocontext) from multiple points of view. This paper discusses a series of relates ALE spanning the disciplines of mathematics, physics, history, and geography. The research described provides a blueprint for an interactive teaching program that bridges multiple departments, introducing students, educators, and faculty to the possibilities of technology-supported integrated curricula learning.},
  keywords={Computer aided instruction;Problem-solving;Computer architecture;Computer science;Materials science and technology;Animation;Computer graphics;Computational modeling;Context modeling;Mathematics},
  doi={10.1109/FIE.1998.738878},
  ISSN={0190-5848},
  month={Nov},}@INPROCEEDINGS{7510353,
  author={Meng Li and Yinghong Wen},
  booktitle={2015 IEEE 6th International Symposium on Microwave, Antenna, Propagation, and EMC Technologies (MAPE)}, 
  title={Applying risk assessment technique to electromagnetic compatibility analysis in Chinese High Speed Railway}, 
  year={2015},
  volume={},
  number={},
  pages={441-445},
  abstract={There are more and more electromagnetic compatibility problems contributing to safety related errors, malfunctions and other failures in Chinese High Speed Railway. However, the classical EMC test and theory does not take into account risk analysis of EMC especially in safety critical systems. In order to help engineers to solve the EMC problems systematically, a method with risk assessment and management thinking is used in this paper. First the study proposes an EMC Risk Analysis Model (EMCRAM) based on the Hierarchical Holographic Modeling (HHM) with four levels. The Analytic Hierarchy Process (AHP) is introduced to calculate the risk of EMC faults when the risk value of the indices is obtained with the method of EMC measurement and experts' knowledge and experience. To demonstrate the approach, it is validated by an application of railway signal system and the results show the EMC risk and vulnerability in the railway signal system.},
  keywords={Electromagnetic interference;Electromagnetics;IEC Standards;Computational modeling;Analytical models;Immunity testing;risk assessment;electromagnetic compatibility;high speed railway},
  doi={10.1109/MAPE.2015.7510353},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10470584,
