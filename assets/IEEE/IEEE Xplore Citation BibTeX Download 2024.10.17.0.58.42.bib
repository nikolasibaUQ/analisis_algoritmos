@INPROCEEDINGS{9330976,
  author={Luo, Xiao and Hu, Yun Qin and Liu, Yue and Mengying, Huang and Chu, Wei and Lin, Jun},
  booktitle={2020 IEEE Vehicle Power and Propulsion Conference (VPPC)}, 
