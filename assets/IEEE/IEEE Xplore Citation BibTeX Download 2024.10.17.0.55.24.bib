  doi={10.1109/ICMSS.2010.5576678},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{5529487,
  author={Yuhua Hu and McLaughlin, Paul},
  booktitle={2010 2nd International Conference on Education Technology and Computer}, 
  title={Technology-enhanced active learning in undergraduate biology education collaborative group work and peer assessment}, 
  year={2010},
  volume={3},
  number={},
  pages={V3-514-V3-518},
  abstract={Teaching of the science subjects in higher education is undergoing a transition from the traditional `chalk and talk' model to a student-centered one that aims to engage students and encourage active learning. Influenced by the social constructivist paradigm, modern teaching promotes collaborative learning and peer assessment as two effective active learning strategies to which technologies are becoming increasingly important assistance. This paper presents three cases in which various technologies were deployed in some undergraduate biology courses to maximize students' active learning opportunities.},
  keywords={Educational technology;Collaborative work;Computer science education;Modems;Biology computing;Peer to peer computing;Computational biology;International collaboration;Electronic mail;Biological system modeling;active learning;collaborative;peer assessment;pedagogy;clickers;biology},
